@model Stark.Models.Cars


@{
    ViewData["Title"] = "SelectPlate";
  	var data=ViewBag.result;
	
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">

        @{

            for (int i = 0; i < data.Count; i++)
            {
                <input type="button" class="selectButtonBefore" id=@i value=@data[i] />
            }
        }
        <label asp-for="Plate" class="control-label"></label>
        <input asp-for="Plate" hidden class="form-control" id="Plate" />
        <span asp-validation-for="Plate" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" formaction=@Url.Action("Create", "Cars") formmethod="post" class="btn btn-default" />
    </div>
</form>


<h2>SelectPlate</h2>

